{
  "name": "Marqueed Screenshots",
  "description": "Simple Screenshot Sharing",
  "version": "0.4",
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "manifest_version": 2,
  "oauth2": {
    "client_id": "440580062850-49411snc1o961f5dvei87c1jsi58u23c.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "permissions": [
    "identity",
  	"fileSystem",
    "clipboardWrite",
    "webview",
    "storage",
    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/",
    "http://*/",
    "https://*/"
  ],
  "icons": { "16": "mq_icon_16.png", "128": "mq_icon_128.png" }
}